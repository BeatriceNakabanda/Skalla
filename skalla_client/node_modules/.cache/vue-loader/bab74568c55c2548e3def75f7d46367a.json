oter,\n      FadeTransition\n    },\n    data() {\n      return {\n        sidebarBackground: 'vue' //vue|blue|orange|green|red|primary\n      };\n    },\n    methods: {\n      toggleSidebar() {\n        if (this.$sidebar.showSidebar) {\n          this.$sidebar.displaySidebar(false);\n        }\n      },\n      logout() {\n      store.dispatch('logout');\n      router.push('/');\n    }\n    }\n  };\n</script>\n<style lang=\"scss\">\n#signout-position{\n  background-color: #5E72E4;\n  padding-left: 28px;\n  padding-top: 15px;\n  padding-bottom: 36px;\n  position: absolute;\n  width: 100%;\n  height: 6%;\n  bottom: 0px;\n  left: 0;\n \n}\n</style>\n"]}]}
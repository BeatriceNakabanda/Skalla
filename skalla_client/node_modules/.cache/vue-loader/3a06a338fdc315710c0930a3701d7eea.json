outer';\n  // import axios from \"axios\"\n\n  export default {\n    components: {\n      ProjectManagerNavbar,\n      ContentFooter,\n      FadeTransition\n    },\n    data() {\n      return {\n        sidebarBackground: 'vue', //vue|blue|orange|green|red|primary\n        role: '',\n        secretMessage: ''\n\n      };\n    },\n    methods: {\n      toggleSidebar() {\n        if (this.$sidebar.showSidebar) {\n          this.$sidebar.displaySidebar(false);\n        }\n        \n      },\n      logout() {\n      this.$store.dispatch('logout');\n      router.push('/');\n    }\n    }\n  };\n</script>\n<style lang=\"scss\">\n\n#signout-position{\n  background-color: #5E72E4;\n  padding-left: 28px;\n  padding-top: 15px;\n  padding-bottom: 36px;\n  position: absolute;\n  width: 100%;\n  height: 6%;\n  bottom: 0px;\n  left: 0;\n \n}\n// .main{\n//   padding-top: 1000px;\n// }\n</style>\n"]}]}